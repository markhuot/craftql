{
    "name": "markhuot/craftql",
    "description": "A GraphQL implementation for Craft",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin"
    ],
    "require": {
        "webonyx/graphql-php": "^0.11.0",
        "react/http": "^0.7",
        "firebase/php-jwt": "^5.0.0"
    },
    "authors": [
        {
            "name": "Mark Huot",
            "email": "mark@markhuot.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "markhuot\\CraftQL\\": "src/"
        }
    },
    "extra": {
        "name": "CraftQL",
        "handle": "craftql",
        "developer": "Mark Huot",
        "developerUrl": "https://www.github.com/markhuot",
        "class": "markhuot\\CraftQL\\CraftQL",
        "components": {
            "graphQl": "markhuot\\CraftQL\\Services\\GraphQLService",
            "jwt": "markhuot\\CraftQL\\Services\\JWTService"
        }
    }
}
